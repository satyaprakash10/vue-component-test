{"ast":null,"code":"import BaseTable from '../components/BaseTable.vue';\nimport { mapActions, mapGetters } from 'vuex';\nimport ChildDemo2 from './ChildDemo2.vue';\nexport default {\n  components: {\n    BaseTable,\n    ChildDemo2\n  },\n  data() {\n    return {\n      rows: this.getUsers,\n      messageFromChild: null,\n      columns: [{\n        label: 'Name',\n        field: 'name'\n      }, {\n        label: 'Email',\n        field: 'email'\n      }, {\n        label: 'Phone',\n        field: 'phone'\n      }]\n    };\n  },\n  computed: {\n    ...mapGetters(['getUsers'])\n  },\n  created() {\n    this.fetchUsersList();\n  },\n  mounted() {\n    setTimeout(() => {\n      console.log(this.getUsers, 'asdf');\n      this.rows = this.getUsers;\n    }, 1000);\n  },\n  methods: {\n    ...mapActions(['fetchUsers']),\n    fetchUsersList() {\n      this.fetchUsers();\n    },\n    loadData(value) {\n      this.messageFromChild = value;\n    },\n    childToChildMethod() {\n      this.$root.$emit('messageFromChildToChild', 'This is the example of child to child communication.');\n    }\n  }\n};","map":{"version":3,"mappings":"AAyBA;AACA;AACA;AAEA;EACAA;IACAC;IACAC;EACA;EAEAC;IACA;MACAC;MACAC;MACAC,UACA;QACAC;QACAC;MACA,GACA;QACAD;QACAC;MACA,GACA;QACAD;QACAC;MACA;IAEA;EACA;EAEAC;IACA;EACA;EAEAC;IACA;EACA;EACAC;IACAC;MACAC;MACA;IACA;EACA;EACAC;IACA;IAEAC;MACA;IACA;IACAC;MACA;IACA;IACAC;MACA,iBACA,2BACA,uDACA;IACA;EACA;AACA","names":["components","BaseTable","ChildDemo2","data","rows","messageFromChild","columns","label","field","computed","created","mounted","setTimeout","console","methods","fetchUsersList","loadData","childToChildMethod"],"sourceRoot":"src/views","sources":["ChildDemo.vue"],"sourcesContent":["<template>\n  <div>\n    <h1 class=\"\">Parent to Child Component Demo</h1>\n\n    <BaseTable\n      :table-data-columns=\"columns\"\n      :table-data-rows=\"rows\"\n      url=\"https://jsonplaceholder.typicode.com/users\"\n    />\n\n    <child-demo-2 @clickToCall=\"loadData\" />\n    <h4\n      class=\"bg-gray-200 mx-auto max-w-lg w-full rouned-md p-4 text-black text-lg mt-12\"\n      v-if=\"messageFromChild\"\n    >\n      {{ messageFromChild }}\n    </h4>\n\n    <button type=\"button\" class=\"btn-grp\" @click=\"childToChildMethod\">\n      Click to show Message\n    </button>\n  </div>\n</template>\n\n<script>\nimport BaseTable from '../components/BaseTable.vue'\nimport { mapActions, mapGetters } from 'vuex'\nimport ChildDemo2 from './ChildDemo2.vue'\n\nexport default {\n  components: {\n    BaseTable,\n    ChildDemo2,\n  },\n\n  data() {\n    return {\n      rows: this.getUsers,\n      messageFromChild: null,\n      columns: [\n        {\n          label: 'Name',\n          field: 'name',\n        },\n        {\n          label: 'Email',\n          field: 'email',\n        },\n        {\n          label: 'Phone',\n          field: 'phone',\n        },\n      ],\n    }\n  },\n\n  computed: {\n    ...mapGetters(['getUsers']),\n  },\n\n  created() {\n    this.fetchUsersList()\n  },\n  mounted() {\n    setTimeout(() => {\n      console.log(this.getUsers, 'asdf')\n      this.rows = this.getUsers\n    }, 1000)\n  },\n  methods: {\n    ...mapActions(['fetchUsers']),\n\n    fetchUsersList() {\n      this.fetchUsers()\n    },\n    loadData(value) {\n      this.messageFromChild = value\n    },\n    childToChildMethod() {\n      this.$root.$emit(\n        'messageFromChildToChild',\n        'This is the example of child to child communication.'\n      )\n    },\n  },\n}\n</script>\n\n<style scoped>\n.btn-grp {\n  padding: 0.5rem;\n  background-color: rgb(153 246 228);\n  border-radius: 0.25rem;\n  font-size: 1rem;\n  color: rgb(6 78 59);\n  cursor: pointer;\n}\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}